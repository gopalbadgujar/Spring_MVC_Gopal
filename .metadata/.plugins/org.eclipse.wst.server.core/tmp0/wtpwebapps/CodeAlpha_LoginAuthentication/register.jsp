<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Register Page</title>
</head>
<body>
<h1>Register User Information Here...</h1>

<form action="reg">
<pre>

User ID      : <input type="number" name="id" placeholder="Enter User ID Here">

User Name    : <input type="text" name="name" placeholder="Enter User Name Here">

User Address : <input type="text" name="address" placeholder="Enter User Address Here">

User Birth   : <input type="date" name="birth" placeholder="Enter User Date of Birth Here">

User Email   : <input type="text" name="email" placeholder="Enter User E-mail Here">

User Mob.No  : <input type="number" name="mobNo" placeholder="Enter User Mobile Number Here">

<button type="submit" value="register">REGISTER</button>


</pre>
</form>

</body>
</html>